<% if(isWinner){ %>

<h4> You won! </h4>


<div class="row">

  <% for(var i = 0; i < currentDeck.length; i++){ %>

  <div class="col-sm-2 col-md-3">
    <div>
    <img class="col-sm-12 col-md-11" src="https://deckofcardsapi.com/static/img/<%= currentDeck[i] %>.png" />
    </div>
  </div>

  <% } %>

</div>




<% } else { %>

<h4>You Lost</h4>

<div class="row">

  <% for(var i = 0; i < currentDeck.length; i++){ %>

  <div class="col-sm-2 col-md-3">
    <div>
    <img class="col-sm-12 col-md-11" src="https://deckofcardsapi.com/static/img/<%= currentDeck[i] %>.png" />
    </div>
  </div>

  <% } %>

</div>


<h4>The winning hand:</h4>
<div class="row">
  <% for(var i = 0; i<deck.length; i++){ %>

  <div class="col-sm-2 col-md-3">
    <div>
    <img class="col-sm-12 col-md-11" src="https://deckofcardsapi.com/static/img/<%= deck[i] %>.png" />
    </div>
  </div>

  <% } %>
</div>





<% } %>
